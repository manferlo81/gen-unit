{
  "name": "gen-unit",
  "version": "0.0.6",
  "description": "A generic unit parser/formatter",
  "keywords": [
    "generic",
    "unit",
    "units",
    "parse",
    "parser",
    "format",
    "formatter"
  ],
  "main": "dist/gen-unit.js",
  "module": "dist/gen-unit.mjs",
  "browser": "dist/gen-unit.umd.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "clean:cache": "rimraf node_modules/.cache/{bundlib,jest}",
    "clean:build": "rimraf dist coverage",
    "clean": "run-p clean:*",
    "lint:test": "eslint __test__/**",
    "lint:src": "eslint src/**",
    "lint": "run-p lint:*",
    "test": "jest --color -w 3",
    "test:watch": "npm test -- --watch-all",
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "prerelease": "run-s clean lint test build",
    "release": "standard-version"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.9",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.32",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "bundlib": "^0.18.4",
    "chokidar": "^3.5.1",
    "eslint": "^7.21.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "author": {
    "name": "Manuel Fern√°ndez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/gen-unit.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/gen-unit/issues"
  },
  "homepage": "https://github.com/manferlo81/gen-unit#readme",
  "license": "MIT"
}
