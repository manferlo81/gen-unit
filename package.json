{
  "name": "gen-unit",
  "description": "A generic units parser/formatter",
  "version": "0.0.2",
  "keywords": [
    "generic",
    "unit",
    "units",
    "parse",
    "parser",
    "format",
    "formatter"
  ],
  "main": "dist/gen-unit.js",
  "module": "dist/gen-unit.mjs",
  "browser": "dist/gen-unit.umd.js",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "bundlib -dw",
    "build": "bundlib",
    "test": "jest --color -w 3",
    "test:watch": "npm test -- --watch-all",
    "clean": "rimraf dist types coverage node_modules/.cache/bundlib node_modules/.cache/jest",
    "lint": "eslint src/** __test__/**",
    "prerelease": "npm run clean && npm run lint && npm test && npm run build",
    "release": "standard-version"
  },
  "bundlib": {
    "min": "browser"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "bundlib": "^0.13.4",
    "chokidar": "^3.3.1",
    "eslint": "^6.8.0",
    "husky": "^4.0.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "files": [
    "dist",
    "types"
  ],
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/gen-unit.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/gen-unit/issues"
  },
  "homepage": "https://github.com/manferlo81/gen-unit#readme",
  "license": "MIT"
}
